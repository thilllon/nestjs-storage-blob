{
  "name": "nestjs-storage-blob",
  "version": "2.0.3",
  "description": "Azure Blob Storage module for Nest.js",
  "author": {
    "email": "thilllon970@gmail.com",
    "name": "thilllon",
    "url": "https://github.com/thilllon"
  },
  "license": "MIT",
  "packageManager": "pnpm@7.22.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/thilllon/nestjs-storage-blob.git"
  },
  "contributors": [
    {
      "name": "thilllon",
      "email": "thilllon970@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/thilllon/nestjs-storage-blob/issues"
  },
  "keywords": [
    "nest",
    "nestjs",
    "storage",
    "sas",
    "blobSAS",
    "containerSAS",
    "accountSAS",
    "azure",
    "presigned url",
    "uplaod",
    "@azure/storage-blob",
    "@nest/azure-storage"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install && chmod +x .husky/*",
    "prebuild": "rm -rf dist",
    "build": "tsup",
    "build:tsc": "tsc",
    "format": "prettier --write --list-different .",
    "lint": "eslint --fix .",
    "test": "jest",
    "prerelease": "pnpm build",
    "release": "pnpm format && pnpm lint && pnpm test && pnpm build && release-it",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100
  },
  "lint-staged": {
    "**/src/**/*": [
      "prettier -w -l",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "push": true,
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": "true"
    }
  },
  "devDependencies": {
    "@azure/storage-blob": "^12.12.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/testing": "^9.2.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "axios": "^1.2.0",
    "commitizen": "^4.2.6",
    "commitlint": "^17.4.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "reflect-metadata": "^0.1.13",
    "release-it": "^15.6.0",
    "rxjs": "^7.5.7",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4",
    "webpack": "^5.0.0"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  }
}
